#include<bits/stdc++.h>
using namespace std;
void stringfunction(string s)
{
    cout<<s;
    for(auto it:s)
    {
        cout<<it<<" ";
    }
    /*if(s.find(1)=='k')
    {
        cout<<"YES";
    }*/
    
}
